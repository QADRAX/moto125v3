{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Marcas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "phone": {
      "type": "string"
    },
    "motos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::moto.moto",
      "mappedBy": "company"
    },
    "url": {
      "type": "string"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "relatedCompanies"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "description": {
      "type": "richtext"
    }
  }
}
