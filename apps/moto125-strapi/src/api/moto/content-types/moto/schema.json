{
  "kind": "collectionType",
  "collectionName": "motos",
  "info": {
    "singularName": "moto",
    "pluralName": "motos",
    "displayName": "Motos",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "moto125Id": {
      "type": "uid",
      "required": true
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "motos"
    },
    "fullName": {
      "type": "string"
    },
    "modelName": {
      "type": "string",
      "required": true
    },
    "motoType": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::moto-type.moto-type",
      "inversedBy": "motos"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "priece": {
      "type": "decimal"
    },
    "year": {
      "type": "integer"
    },
    "fichaTecnica": {
      "type": "json"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "relatedMotos"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "normativa": {
      "type": "enumeration",
      "enum": [
        "Euro 1",
        "Euro 2",
        "Euro 3",
        "Euro 4",
        "Euro 5",
        "Euro 5plus"
      ]
    },
    "engineType": {
      "type": "enumeration",
      "enum": [
        "combustión",
        "eléctrico",
        "hibrido"
      ]
    }
  }
}
