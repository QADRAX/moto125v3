{
  "name": "@moto125/data-mirror",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "imports": {
    "#resolveWorker": {
      "require": "./dist/cjs/resolveWorker.cjs.js",
      "import": "./dist/esm/resolveWorker.esm.js"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && node ./scripts/postbuild.mjs",
    "clean": "rimraf dist",
    "dev": "tsc -w",
    "lint": "echo \"(lint shared)\""
  },
  "dependencies": {
    "@moto125/data-mirror-core": "*",
    "@moto125/data-mirror-worker": "*",
    "cron-parser": "^5.4.0"
  }
}